{
    "plugins": [
        "react-redux"
    ],
    "extends": [
        "airbnb", "plugin:react-redux/recommended"
    ],
    "rules": {
        "func-names": ["error", "never"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "env": {
        "jest": true,
        "browser": true
    },
    "globals": {
        "trans": true
    }
}
